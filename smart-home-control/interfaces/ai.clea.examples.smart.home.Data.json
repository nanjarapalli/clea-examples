{
    "interface_name": "ai.clea.examples.smart.home.Data",
    "version_major": 0,
    "version_minor": 1,
    "type": "datastream",
    "ownership": "device",
    "mappings": [
        {
            "endpoint": "/dashboard/fan/speed",
            "type": "integer",
            "reliability": "guaranteed"
        },
        {
            "endpoint": "/dashboard/fan/status",
            "type": "boolean",
            "reliability": "guaranteed",
            "explicit_timestamp": true
        },
        {
            "endpoint": "/dashboard/light/brightness",
            "type": "integer",
            "reliability": "guaranteed"
        },
        {
            "endpoint": "/dashboard/light/dimming",
            "type": "boolean",
            "reliability": "guaranteed"
        },
        {
            "endpoint": "/dashboard/light/green",
            "type": "boolean",
            "reliability": "guaranteed"
        },
        {
            "endpoint": "/dashboard/light/red",
            "type": "boolean",
            "reliability": "guaranteed"
        }
    ]
}
