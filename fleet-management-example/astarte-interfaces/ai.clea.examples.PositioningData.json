{
    "interface_name": "ai.clea.examples.PositioningData.json",
    "version_major": 0,
    "version_minor": 1,
    "type": "datastream",
    "ownership": "device",
    "aggregation": "object",
    "mappings": [
        {
            "endpoint": "/%{sensor_id}/latitude",
            "type": "double",
            "database_retention_policy": "use_ttl",
            "database_retention_ttl": 115200,
            "documentation":"EE current latitude (deg)"
        },
        {
            "endpoint": "/%{sensor_id}/longitude",
            "type": "double",
            "database_retention_policy": "use_ttl",
            "database_retention_ttl": 115200,
            "documentation":"EE current longitude (deg)"
        },
        {
            "endpoint": "/%{sensor_id}/altitude",
            "type": "double",
            "database_retention_policy": "use_ttl",
            "database_retention_ttl": 115200,
            "documentation":"EE current altitude (deg)"
        },
        {
            "endpoint": "/%{sensor_id}/track",
            "type": "double",
            "database_retention_policy": "use_ttl",
            "database_retention_ttl": 115200,
            "documentation":"EE current direction relative to true north (deg)"
        },
        {
            "endpoint": "/%{sensor_id}/speed",
            "type": "double",
            "database_retention_policy": "use_ttl",
            "database_retention_ttl": 115200,
            "documentation":"EE current speed over ground (m/s)"
        },
        {
            "endpoint": "/%{sensor_id}/climb",
            "type": "double",
            "database_retention_policy": "use_ttl",
            "database_retention_ttl": 115200,
            "documentation":"EE current vertical speed (m/s)"
        }
    ]
}
