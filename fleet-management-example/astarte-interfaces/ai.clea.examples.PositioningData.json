{
    "interface_name": "ai.clea.examples.PositioningData",
    "version_major": 0,
    "version_minor": 1,
    "type": "datastream",
    "ownership": "device",
    "aggregation": "object",
    "mappings": [
        {
            "endpoint": "/latitude",
            "type": "double",
            "database_retention_policy": "use_ttl",
            "database_retention_ttl": 31536000,
            "documentation":"EE current latitude (deg)"
        },
        {
            "endpoint": "/longitude",
            "type": "double",
            "database_retention_policy": "use_ttl",
            "database_retention_ttl": 31536000,
            "documentation":"EE current longitude (deg)"
        },
        {
            "endpoint": "/altitude",
            "type": "double",
            "database_retention_policy": "use_ttl",
            "database_retention_ttl": 31536000,
            "documentation":"EE current altitude (deg)"
        },
        {
            "endpoint": "/track",
            "type": "double",
            "database_retention_policy": "use_ttl",
            "database_retention_ttl": 31536000,
            "documentation":"EE current direction relative to true north (deg)"
        },
        {
            "endpoint": "/speed",
            "type": "double",
            "database_retention_policy": "use_ttl",
            "database_retention_ttl": 31536000,
            "documentation":"EE current speed over ground (m/s)"
        },
        {
            "endpoint": "/climb",
            "type": "double",
            "database_retention_policy": "use_ttl",
